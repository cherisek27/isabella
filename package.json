{
  "name": "isabella",
  "version": "1.0.0",
  "description": "isabella lombardini website",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:prod && serve dist",
    "dev": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --history-api-fallback",
    "build:prod": "webpack -p && cp src/index.html dist/index.html && cp -r src/assets dist/assets"
  },
  "keywords": [
    "reactjs"
  ],
  "author": "cherise knox",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "lightbox2": "^2.10.0",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-bootstrap-carousel": "^2.2.1",
    "react-dom": "^16.2.0",
    "react-image-lightbox": "^4.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slideshow-image": "^0.1.3",
    "serve": "^6.4.8"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.8",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  }
}
